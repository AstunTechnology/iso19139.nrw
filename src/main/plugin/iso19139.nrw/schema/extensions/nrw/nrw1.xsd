<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xlink="http://www.w3.org/1999/xlink" 
	xmlns:gml="http://www.opengis.net/gml/3.2" 
	xmlns:gmd="http://www.isotc211.org/2005/gmd" 
	xmlns:gco="http://www.isotc211.org/2005/gco" 
	xmlns:nrw="http://naturalresources.wales/nrw" 
	targetNamespace="http://naturalresources.wales/nrw" elementFormDefault="qualified" version="1.0">
	<xs:annotation>
		<xs:documentation>Schema for Natural Resources Wales Metadata Profile v1</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://www.isotc211.org/2005/gmd" schemaLocation="../../gmd/gmd.xsd"/>
	<xs:import namespace="http://www.isotc211.org/2005/gco" schemaLocation="../../gco/gco.xsd"/>
	<xs:import namespace="http://www.isotc211.org/2005/srv" schemaLocation="../../srv/srv.xsd"/>
	<xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="../../gml/gml.xsd"/>
	<!-- =========================================================================== -->

	<xs:annotation>
		<xs:documentation>Extensions to hold additional NRW elements</xs:documentation>
	</xs:annotation>

	<!-- Define element for Internal Contact Info element. Extends gmd:identificationInfo -->
	<xs:element name="internalContactInfo" minOccurs="0" maxOccurs="1">
	<xs:annotation>
		<xs:documentation>Extension to hold NRW Internal Contact Information</xs:documentation>
	</xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="gco:CharacterString"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="internalLocationInfo" minOccurs="0" maxOccurs="1">
	<xs:annotation>
		<xs:documentation>Extension to hold NRW Internal Location Information</xs:documentation>
	</xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="gco:CharacterString"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>



</xs:schema>